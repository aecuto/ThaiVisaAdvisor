<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Visa exp</th>
      <th>Wp exp</th>
      <th>Noti90</th>
      <th>Passport exp</th>
      <th>Doe</th>
      <th>Dob</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.full_name %></td>
        <td><%= member.visa_exp %></td>
        <td><%= member.wp_exp %></td>
        <td><%= member.noti90 %></td>
        <td><%= member.passport_exp %></td>
        <td><%= member.doe %></td>
        <td><%= member.dob %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
